import math
def check_fib(num):   
    a = ((5*num*num)+4)   #This is one of the properties of Fabonacci Series.
    
    b = int()
    b = math.sqrt(a)      #This returns the Square root of the number.

    if ((b-math.floor(b)))== 0: #This math.floor function is used to return the closest integer value which is less than or equal to the specified expression or Value.
        return True
    else :
        return False
